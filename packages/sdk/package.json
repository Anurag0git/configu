    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf tsconfig.tsbuildinfo build",
    "build": "tsc --build",
    "bundle": "tsup"
  },
  "dependencies": {
    "@apideck/better-ajv-errors": "^0.3.6",
    "adaptive-expressions": "^4.23.1",
    "ajv": "^8.17.1",
    "lodash": "^4.17.21",
    "parse-json": "8.1.0",
    "type-fest": "^4.26.1"
  },
  "devDependencies": {}
