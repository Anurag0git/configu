{
  "name": "@configu/proxy",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Configu Proxy Server",
  "author": "@configu/dev",
  "scripts": {
    "dev": "tsx watch --inspect --clear-screen=false src/index.ts",
    "build:python": "openapi-generator-cli generate -i configuproxy.json -g python -o sdk/python --package-name 'configu_proxy'",
    "build:javascript": "openapi-generator-cli generate -i configuproxy.json -g javascript -o sdk/javascript --package-name '@configu/proxy'"
  },
  "engines": {
    "node": ">=v20.0.0"
  },
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "/bin",
    "/build/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/configu/configu",
    "directory": "packages/proxy"
  },
  "bugs": {
    "url": "https://github.com/configu/configu/issues",
    "email": "dev@configu.com"
  },
  "homepage": "https://configu.com",
  "keywords": [
    "cfgu",
    "configu",
    "configu-proxy",
    "configu-proxy-server",
    "configu-server"
  ],
  "dependencies": {
    "@configu/lib": "^0.7.3",
    "@configu/node": "^0.24.0",
    "@configu/ts": "workspace:*",
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@scalar/fastify-api-reference": "^1.24.27",
    "cosmiconfig": "^9.0.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "json-schema-to-ts": "^3.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "fastify-cli": "^6.2.1"
  }
}
