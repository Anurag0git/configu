    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf tsconfig.tsbuildinfo build",
    "build": "tsc --build",
    "bundle": "tsup"
  },
  "dependencies": {
    "@configu/node": "workspace:*",
    "@configu/ts": "workspace:*",
    "@configu/sdk": "workspace:*",
    "change-case": "4.1.2",
    "cosmiconfig": "^9.0.0",
    "lodash": "^4.17.21",
    "type-fest": "^4.8.3"
  },
  "devDependencies": {}
